<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Demo Faqs</title>
    @*<link href="~/Content/Site.css" rel="stylesheet" type="text/css" />*@
    @*<link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />*@
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Content/global.css" rel="stylesheet" type="text/css" />
    @Html.Raw(ViewBag.customCss)
</head>
<body>
    <header>
        <div class="page-wrapper">
            <h1>@Html.ActionLink("FAQ Demo", "Index", "Home", null, null)</h1>
            <nav id="header-nav" class="page-wrapper">
                <button>Admin</button> | <button>Logout</button>
            </nav>
        </div>
    </header>
    <main>@RenderBody()</main>
    <footer>
        <p class="page-wrapper">&copy; @DateTime.Now.Year - Lee</p>
    </footer>
</body>
</html>